<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  
  <parent>
    <artifactId>sitech-strategy-main</artifactId>
    <groupId>com.sitech.strategy</groupId>
    <version>0.0.1-SNAPSHOT</version>
  </parent>
  
  <groupId>com.sitech.strategy</groupId>
  <artifactId>sitech-strategy-recommend</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>sitech-strategy-recommend</name>
  <url>http://maven.apache.org</url>
  
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  
  <dependencies>
  	<dependency>
	    <groupId>org.apache.spark</groupId>
	    <artifactId>spark-core_2.10</artifactId>
	    <version>1.6.1</version>
	       	<exclusions>
	    		<exclusion>
					<groupId>org.slf4j</groupId>
    				<artifactId>jcl-over-slf4j</artifactId>
	    		</exclusion>
    		</exclusions> 
	</dependency>
	<dependency>
	    <groupId>org.apache.spark</groupId>
	    <artifactId>spark-streaming_2.10</artifactId>
	    <version>1.6.1</version>
	</dependency>
	<dependency>
    	<groupId>org.apache.spark</groupId>
    	<artifactId>spark-hive_2.10</artifactId>
    	<version>1.6.1</version>
    	    <exclusions>
	    		<exclusion>
					<groupId>org.slf4j</groupId>
    				<artifactId>jcl-over-slf4j</artifactId>
	    		</exclusion>
    		</exclusions> 
	</dependency>
	<dependency>
	    <groupId>org.apache.spark</groupId>
	    <artifactId>spark-streaming-kafka_2.10</artifactId>
	    <version>1.6.1</version>
	</dependency>
	<dependency>
  		<groupId>com.sitech.strategy</groupId>
  		<artifactId>sitech-strategy-core</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
	</dependency>
  	<dependency>
  		<groupId>com.sitech.strategy</groupId>
  		<artifactId>sitech-strategy-persist</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
	</dependency>
  	<dependency>
  		<groupId>com.sitech.strategy</groupId>
  		<artifactId>sitech-strategy-common</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
	</dependency>
  </dependencies>
  
  <build>
	  <resources>
	    <resource>
	       <directory>${project.basedir}/src/main/resources</directory>
	       <filtering>true</filtering>
	    </resource>
	  </resources>
	  <testResources>
	    <testResource>
	       <directory>${project.basedir}/src/test/resources</directory>
	       <filtering>true</filtering>
	    </testResource>
	  </testResources>
  </build>
  
  <profiles>
  	<profile>
  		<id>local</id>
  		<properties>
  			<parseRecommendEntity>com.sitech.strategy.recommend.entity.impl.ParseRecommendEntity</parseRecommendEntity>
  			<parseRecommendParam>com.sitech.strategy.recommend.entity.param.impl.ParseRecommendParam</parseRecommendParam>
  			<eventReceiverParam>com.sitech.strategy.recommend.entity.param.impl.EventReceiverParam</eventReceiverParam>
  			<batchPushTaskParam>com.sitech.strategy.recommend.entity.param.impl.BatchPushTaskParam</batchPushTaskParam>
  			<batchTaskRecommendEntity>com.sitech.strategy.recommend.entity.impl.BatchTaskRecommendEntity</batchTaskRecommendEntity>
  		</properties>
  	</profile>
  	<profile>
  		<id>jlmobtest</id>
  		<properties>
  			<parseRecommendEntity>com.sitech.strategy.recommend.entity.impl.ParseRecommendEntity</parseRecommendEntity>
  			<parseRecommendParam>com.sitech.strategy.recommend.entity.param.impl.extend.ParseRecommendParamByJLMob</parseRecommendParam>
  			<eventReceiverParam>com.sitech.strategy.recommend.entity.param.impl.extend.EventReceiverParamByJLMob</eventReceiverParam>
  			<batchPushTaskParam>com.sitech.strategy.recommend.entity.param.impl.BatchPushTaskParam</batchPushTaskParam>
  			<batchTaskRecommendEntity>com.sitech.strategy.recommend.entity.impl.BatchTaskRecommendEntity</batchTaskRecommendEntity>
  		</properties>
  		<activation>
            <activeByDefault>true</activeByDefault>
        </activation>
  	</profile> 	 
  	<profile>
  		<id>jlmobprod</id>
  		<properties>
  			<parseRecommendEntity>com.sitech.strategy.recommend.entity.impl.ParseRecommendEntity</parseRecommendEntity>
  			<parseRecommendParam>com.sitech.strategy.recommend.entity.param.impl.extend.ParseRecommendParamByJLMob</parseRecommendParam>
  			<eventReceiverParam>com.sitech.strategy.recommend.entity.param.impl.extend.EventReceiverParamByJLMob</eventReceiverParam>
  			<batchPushTaskParam>com.sitech.strategy.recommend.entity.param.impl.BatchPushTaskParam</batchPushTaskParam>
  			<batchTaskRecommendEntity>com.sitech.strategy.recommend.entity.impl.BatchTaskRecommendEntity</batchTaskRecommendEntity>
  		</properties>
  	</profile>
  	<profile>
  		<id>jlmobgray</id>
  		<properties>
  			<parseRecommendEntity>com.sitech.strategy.recommend.entity.impl.ParseRecommendEntity</parseRecommendEntity>
  			<parseRecommendParam>com.sitech.strategy.recommend.entity.param.impl.extend.ParseRecommendParamByJLMob</parseRecommendParam>
  			<eventReceiverParam>com.sitech.strategy.recommend.entity.param.impl.extend.EventReceiverParamByJLMob</eventReceiverParam>
  			<batchPushTaskParam>com.sitech.strategy.recommend.entity.param.impl.BatchPushTaskParam</batchPushTaskParam>
  			<batchTaskRecommendEntity>com.sitech.strategy.recommend.entity.impl.BatchTaskRecommendEntity</batchTaskRecommendEntity>
  		</properties>
  	</profile> 	 	
  </profiles>
</project>
