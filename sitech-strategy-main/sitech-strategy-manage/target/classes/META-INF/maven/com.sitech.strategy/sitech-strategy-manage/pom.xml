<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  
  <parent>
    <artifactId>sitech-strategy-main</artifactId>
    <groupId>com.sitech.strategy</groupId>
    <version>0.0.1-SNAPSHOT</version>
  </parent>
  
  <groupId>com.sitech.strategy</groupId>
  <artifactId>sitech-strategy-manage</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <name>sitech-strategy-manage</name>
  <url>http://maven.apache.org</url>
  
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  
  <dependencies>
    <dependency>
  		<groupId>com.sitech.strategy</groupId>
  		<artifactId>sitech-strategy-core</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
	</dependency>
  	<dependency>
  		<groupId>com.sitech.strategy</groupId>
  		<artifactId>sitech-strategy-persist</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
	</dependency>
  	<dependency>
  		<groupId>com.sitech.strategy</groupId>
  		<artifactId>sitech-strategy-common</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
	</dependency>
  </dependencies>
  
  <build>
	  <resources>
	    <resource>
	       <directory>${project.basedir}/src/main/resources</directory>
	       <filtering>true</filtering>
	    </resource>
	  </resources>
	  <testResources>
	    <testResource>
	       <directory>${project.basedir}/src/test/resources</directory>
	       <filtering>true</filtering>
	    </testResource>
	  </testResources>
  </build>
  
    <profiles>
	<profile>
  		<id>local</id>
  		<properties>
  			<custGroupManageParam>com.sitech.strategy.manage.entity.param.impl.CustGroupManageParam</custGroupManageParam>
  			<marketCaseManageParam>com.sitech.strategy.manage.entity.param.impl.MarketCaseManageParam</marketCaseManageParam>
  			<userManageParam>com.sitech.strategy.manage.entity.param.impl.UserManageParam</userManageParam>
  		</properties>
  	</profile>
  	<profile>
  		<id>oracledev</id>
  		<properties>
  			<custGroupManageParam>com.sitech.strategy.manage.entity.param.impl.CustGroupManageParam</custGroupManageParam>
  			<marketCaseManageParam>com.sitech.strategy.manage.entity.param.impl.MarketCaseManageParam</marketCaseManageParam>
  			<userManageParam>com.sitech.strategy.manage.entity.param.impl.UserManageParam</userManageParam>
  		</properties>
  	</profile>
  	<profile>
  		<id>mysqldev</id>
  		<properties>
  			<custGroupManageParam>com.sitech.strategy.manage.entity.param.impl.CustGroupManageParam</custGroupManageParam>
  			<marketCaseManageParam>com.sitech.strategy.manage.entity.param.impl.MarketCaseManageParam</marketCaseManageParam>
  			<userManageParam>com.sitech.strategy.manage.entity.param.impl.UserManageParam</userManageParam>
  		</properties>
  	</profile>
  	<profile>
  		<id>jlmobtest</id>
  		<properties>
  			<custGroupManageParam>com.sitech.strategy.manage.entity.param.impl.extend.CustGroupManageParamByJLMob</custGroupManageParam>
  			<marketCaseManageParam>com.sitech.strategy.manage.entity.param.impl.extend.MarketCaseManageParamByJLMob</marketCaseManageParam>
  			<userManageParam>com.sitech.strategy.manage.entity.param.impl.extend.UserManageParamByJLMob</userManageParam>
  		</properties>
  		<activation>
            <activeByDefault>true</activeByDefault>
        </activation>
  	</profile>
  	<profile>
  		<id>jlmobprod</id>
  		<properties>
  			<custGroupManageParam>com.sitech.strategy.manage.entity.param.impl.extend.CustGroupManageParamByJLMob</custGroupManageParam>
  			<marketCaseManageParam>com.sitech.strategy.manage.entity.param.impl.extend.MarketCaseManageParamByJLMob</marketCaseManageParam>
  			<userManageParam>com.sitech.strategy.manage.entity.param.impl.extend.UserManageParamByJLMob</userManageParam>
  		</properties>
  	</profile>
  	<profile>
  		<id>jlmobgray</id>
  		<properties>
  			<custGroupManageParam>com.sitech.strategy.manage.entity.param.impl.extend.CustGroupManageParamByJLMob</custGroupManageParam>
  			<marketCaseManageParam>com.sitech.strategy.manage.entity.param.impl.extend.MarketCaseManageParamByJLMob</marketCaseManageParam>
  			<userManageParam>com.sitech.strategy.manage.entity.param.impl.extend.UserManageParamByJLMob</userManageParam>
  		</properties>
  	</profile>
  </profiles>
</project>
