<?xml version="1.0"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  
  <parent>
    <artifactId>sitech-strategy-main</artifactId>
    <groupId>com.sitech.strategy</groupId>
    <version>0.0.1-SNAPSHOT</version>
  </parent>
  
  <groupId>com.sitech.strategy</groupId>
  <artifactId>sitech-strategy-site</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <packaging>war</packaging>
  <name>sitech-strategy-site Maven Webapp</name>
  <url>http://maven.apache.org</url>
  
  <dependencies>
  	<dependency>
    	<groupId>joda-time</groupId>
    	<artifactId>joda-time</artifactId>
    	<version>2.3</version>
	</dependency>
  	<dependency>
    	<groupId>commons-dbcp</groupId>
    	<artifactId>commons-dbcp</artifactId>
    	<version>1.3</version>
	</dependency>
	<dependency>
    	<groupId>org.springframework</groupId>
    	<artifactId>spring-orm</artifactId>
    	<version>3.0.5.RELEASE</version>
	</dependency>
  	<dependency>
    	<groupId>org.springframework</groupId>
    	<artifactId>spring-tx</artifactId>
    	<version>3.0.5.RELEASE</version>
	</dependency>
	<dependency>
    	<groupId>org.springframework</groupId>
    	<artifactId>spring-web</artifactId>
    	<version>3.0.5.RELEASE</version>
	</dependency>
	<dependency>
    	<groupId>org.jboss.resteasy</groupId>
    	<artifactId>resteasy-jaxrs</artifactId>
    	<version>2.3.4.Final</version>
    	<exclusions>
    		<exclusion>
    			<groupId>commons-codec</groupId>
    			<artifactId>commons-codec</artifactId>
    		</exclusion>
    	</exclusions>
	</dependency>
	<dependency>
  		<groupId>com.sitech.ac.brm</groupId>
  		<artifactId>sitech-ac-brmAgent</artifactId>
  		<version>1.0.0</version>
	</dependency>
	<dependency>
  		<groupId>com.sitech.ac.hsf</groupId>
  		<artifactId>sitech-ac-hsf</artifactId>
  		<version>2.0.2</version>
	</dependency>
	<dependency>
	    <groupId>org.apache.axis2</groupId>
	    <artifactId>axis2</artifactId>
	    <version>1.6.2</version>
	</dependency>
	<dependency>
	    <groupId>org.apache.axis2</groupId>
	    <artifactId>axis2-spring</artifactId>
	    <version>1.6.2</version>
	</dependency>
	<dependency>
	    <groupId>org.apache.axis2</groupId>
	    <artifactId>axis2-transport-http</artifactId>
	    <version>1.6.2</version>
	</dependency>
	<dependency>
	    <groupId>org.apache.axis2</groupId>
	    <artifactId>axis2-transport-local</artifactId>
	    <version>1.6.2</version>
	</dependency>
	<dependency>
	    <groupId>org.apache.axis2</groupId>
	    <artifactId>axis2-xmlbeans</artifactId>
	    <version>1.6.2</version>
	</dependency>
	<dependency>
	    <groupId>wsdl4j</groupId>
	    <artifactId>wsdl4j</artifactId>
	    <version>1.6.3</version>
	</dependency>
  	<dependency>
  		<groupId>com.sitech.strategy</groupId>
  		<artifactId>sitech-strategy-manage</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
	</dependency>
	<dependency>
  		<groupId>com.sitech.strategy</groupId>
  		<artifactId>sitech-strategy-recommend</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
	</dependency>
	<dependency>
  		<groupId>com.sitech.strategy</groupId>
  		<artifactId>sitech-strategy-event</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
	</dependency>
	<dependency>
  		<groupId>com.sitech.strategy</groupId>
  		<artifactId>sitech-strategy-process</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
	</dependency>
  	<dependency>
  		<groupId>com.sitech.strategy</groupId>
  		<artifactId>sitech-strategy-core</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
	</dependency>
  	<dependency>
  		<groupId>com.sitech.strategy</groupId>
  		<artifactId>sitech-strategy-persist</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
	</dependency>
  	<dependency>
  		<groupId>com.sitech.strategy</groupId>
  		<artifactId>sitech-strategy-common</artifactId>
  		<version>0.0.1-SNAPSHOT</version>
	</dependency>
  </dependencies>
  
  <build>
	  <finalName>sitech-strategy-site</finalName>
	  <resources>
	    <resource>
	       <directory>${project.basedir}/src/main/resources</directory>
	       <filtering>true</filtering>
	    </resource>
	  </resources>
	  <testResources>
	    <testResource>
	       <directory>${project.basedir}/src/test/resources</directory>
	       <filtering>true</filtering>
	    </testResource>
	  </testResources>
  </build>
  
    <profiles>
	<profile>
  		<id>local</id>
  		<properties>
  			<default.service.mode>rest</default.service.mode>
  		</properties>
  		<activation>
            <activeByDefault>true</activeByDefault>
        </activation>
  	</profile>
  	<profile>
  		<id>oracledev</id>
  		<properties>
  			<default.service.mode>rest</default.service.mode>
  		</properties>
  	</profile>
  	<profile>
  		<id>mysqldev</id>
  		<properties>
  			<default.service.mode>rest</default.service.mode>
  		</properties>
  	</profile>
  	<profile>
  		<id>dbcpmysqldev</id>
  		<properties>
  			<default.service.mode>rest</default.service.mode>
  		</properties>
  	</profile>

  	<profile>
  		<id>jlmobtest</id>
  		<properties>
  			<default.service.mode>hsf</default.service.mode>
  			<hsf.consumerAppName>strategy</hsf.consumerAppName>
  			<hsf.zookeeper>10.163.106.152\:2180,10.163.106.152\:2181,10.163.106.152\:2182</hsf.zookeeper>
  			<hsf.provider.protocol>dubbo</hsf.provider.protocol>
  		</properties>
  		<activation>
            <activeByDefault>true</activeByDefault>
        </activation>
  	</profile>
 	<profile>
  		<id>jlmobgray</id>
  		<properties>
  			<default.service.mode>hsf</default.service.mode>
  			<hsf.consumerAppName>strategy</hsf.consumerAppName>
  			<hsf.zookeeper>10.162.200.25:2181,10.162.200.25:2182,10.162.200.25:2183</hsf.zookeeper>
  			<hsf.provider.protocol>dubbo</hsf.provider.protocol>
  		</properties>

  	</profile>
  </profiles>
  
</project>
